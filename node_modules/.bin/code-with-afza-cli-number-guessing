#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir//usr/bin/enov" ]; then
  exec "$basedir//usr/bin/enov" node "$basedir/../code-with-afza-cli-number-guessing/index.js" "$@"
else 
  exec /usr/bin/enov node "$basedir/../code-with-afza-cli-number-guessing/index.js" "$@"
fi
